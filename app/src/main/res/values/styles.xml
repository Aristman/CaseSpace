<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="BottomSheet">
        <item name="android:background">?colorSecondaryVariant</item>
    </style>

    <style name="SettingTitle">
        <item name="android:textAppearance">@style/TextAppearance.MaterialComponents.Headline5
        </item>
        <item name="android:gravity">center</item>
    </style>

    <style name="SettingCard" parent="Widget.MaterialComponents.CardView">
        <item name="cardElevation">4dp</item>
        <item name="cardCornerRadius">20dp</item>
    </style>

    <style name="Button">
        <item name="android:elevation">@dimen/default_elevation</item>
    </style>

    <style name="Button.Confirm" parent="Widget.MaterialComponents.Button.OutlinedButton">

    </style>

    <style name="MarRoverTextTitle" parent="TextAppearance.MaterialComponents.Headline6">
        <item name="android:textColor">?colorOnPrimary</item>
    </style>

    <style name="MarRoverText" parent="TextAppearance.MaterialComponents.Headline5">
        <item name="android:textColor">?colorOnSecondary</item>
    </style>

    <style name="NoteTitle" parent="TextAppearance.MaterialComponents.Headline5">
        <item name="android:textAlignment">center</item>
    </style>
</resources>